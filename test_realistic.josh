# Test with realistic coordinates (similar to working examples)

start unit m
  alias meter
  alias meters
end unit

start unit count
end unit

start simulation Main
  # Use realistic coordinates matching the GeoTIFF
  # GeoTIFF bounds: west=-118.03, south=34.0, east=-118.0, north=34.03
  # grid.low = NW corner (top-left), grid.high = SE corner (bottom-right)
  grid.size = 100 m
  grid.low = 34.03 degrees latitude, -118.03 degrees longitude
  grid.high = 34.0 degrees latitude, -118.0 degrees longitude
  grid.patch = "Default"

  steps.low = 0 count
  steps.high = 1 count

  exportFiles.patch = "file:///workspace/results/test_realistic_{replicate}.csv"
end simulation

start patch Default
  fireSeverity.init = external data
  export.fireSeverity.init = fireSeverity
end patch
