# Test: organism attribute update without prior.

start unit count
end unit

start unit m
  alias meter
  alias meters
end unit

start simulation TestNoPrior
  grid.size = 10 m
  grid.low = 0 m latitude, 0 m longitude
  grid.high = 30 m latitude, 30 m longitude
  steps.low = 0 count
  steps.high = 10 count
  exportFiles.patch = "file:///workspace/results/test_no_prior_{replicate}.csv"
end simulation

start patch Default
  Thing.init = create Thing
  Thing.step = prior.Thing
  export.sumAge.step = sum(Thing.age)
end patch

start organism Thing
  age.init = 0 count
  # Try without prior. prefix - just reference age directly
  age.step = age + 1 count
end organism
